<section
  data-id="{{ uniqueId }}"
  class="veda-lazyload pos:relative z:1 bgz:cover bgp:center bgc:color-light c:color-gray7 {{ section.settings["color_scheme"] }}"
  data-scroll-id="veda-product-information"
  style="{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }}{{ sectionSeparator }}"
  data-unavailable-text="{{ 'veda.products.product.unavailable' | t }}"
  data-available-text="{{ 'veda.products.product.add_to_cart' | t }}"
>
  <div
    class="{{ containerType }}"
    {% if section.settings.VEDA_slider_1
      or section.settings.VEDA_slider_1 == 0
      or section.settings.section_settings___width
      or section.settings.section_settings___width == 0
    %}
      style="{{ containerGap }}{{ containerWidth }}"
    {% endif %}
  >
    <div
      class="row"
      {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
        style="{{ rowGap }};--media-radius: {{ section.settings["VEDA_slider_9"] }}px"
      {% else %}
        style="--media-radius: {{ section.settings["VEDA_slider_9"] }}px"
      {% endif %}
    >
      <div
        {% if section.settings.product_media___position == 'left' %}
          class="d:flex fld:row fld:column@+md"
        {% else %}
          class="d:flex fld:row-reverse fld:column@+md"
        {% endif %}
        data-css="{{ section.settings["container_design__id"] }}"
      >
        {% if product %}
          {% if builderMode != true %}
            {% liquid
              if product.selected_or_first_available_variant.featured_media
                assign seo_media = product.selected_or_first_available_variant.featured_media
              else
                assign seo_media = product.featured_media
              endif

              assign first_3d_model = product.media | where: 'media_type', 'model' | first
            %}
            {% if first_3d_model %}
              <script type="application/json" id="ProductJSON-{{ product.id }}">
                {{ product.media | where: 'media_type', 'model' | json }}
              </script>
            {% endif %}
            <script type="application/ld+json">
              {
                "@context": "http://schema.org/",
                "@type": "Product",
                "name": {{ product.title | json }},
                "url": {{ request.origin | append: product.url | json }},
                {% if seo_media %}
                  "image": [
                    {{ seo_media | image_url: width: 1920 | prepend: "https:" | json }}
                  ],
                {% endif %}
                "description": {{ product.description | strip_html | json }},
                {% if product.selected_or_first_available_variant.sku != blank %}
                  "sku": {{ product.selected_or_first_available_variant.sku | json }},
                {% endif %}
                "brand": {
                  "@type": "Brand",
                  "name": {{ product.vendor | json }}
                },
                "offers": [
                  {% for seoVariant in product.variants %}
                    {
                      "@type" : "Offer",
                      {% if seoVariant.sku != blank %}
                        "sku": {{ seoVariant.sku | json }},
                      {% endif %}
                      {% if seoVariant.barcode.size == 12 %}
                        "gtin12": {{ seoVariant.barcode }},
                      {% endif %}
                      {% if seoVariant.barcode.size == 13 %}
                        "gtin13": {{ seoVariant.barcode }},
                      {% endif %}
                      {% if seoVariant.barcode.size == 14 %}
                        "gtin14": {{ seoVariant.barcode }},
                      {% endif %}
                      "availability" : "http://schema.org/{% if seoVariant.available %}InStock{% else %}OutOfStock{% endif %}",
                      "price" : {{ seoVariant.price | divided_by: 100.00 | json }},
                      "priceCurrency" : {{ cart.currency.iso_code | json }},
                      "url" : {{ request.origin | append: seoVariant.url | json }}
                    }{% unless forloop.last %},{% endunless %}
                  {% endfor %}
                ]
              }
            </script>
          {% endif %}
          {{ Image }}
          {{ Content }}
        {% else %}
          <h2 class="ta:center">Please add a product at least to your shop to use this section</h2>
        {% endif %}
      </div>
    </div>
  </div>
  {% if section.settings.VEDA_select_2 == 'video' and section.settings.VEDA_video_1 %}
    <div data-veda-video="{{ section.settings["VEDA_video_1"] }}"></div>
  {% endif %}
  {% if section.settings.VEDA_switch_2 %}
    <div
      class="pos:absolute ins:0 z:-1"
      style="background-color: {{ section.settings["VEDA_color_2"] }};backdrop-filter: blur({{ section.settings["VEDA_slider_3"] }}px)"
    ></div>
  {% endif %}
</section>
