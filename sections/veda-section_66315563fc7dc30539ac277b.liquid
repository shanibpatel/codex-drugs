                "id": {{ product.selected_or_first_available_variant.id | json }},
                "available": {{ product.selected_or_first_available_variant.available | default: false | json }},
                "price": {{ product.selected_or_first_available_variant.price | money | json }},
                "veda_price": {{ product.selected_or_first_available_variant.price | json }},
                {% if product.selected_or_first_available_variant.compare_at_price %}
                  "compare_at_price": {{ product.selected_or_first_available_variant.compare_at_price | money | json }},
                  "veda_compare_at_price": {{ product.selected_or_first_available_variant.compare_at_price | json }},
                {% else %}
                  "compare_at_price": {{ 0 | money | json }},
                  "veda_compare_at_price": {{ 0 | json }},
                {% endif %}
                "featured_image": {{ product.selected_or_first_available_variant.featured_image | json }},
                "image": {{ product.selected_or_first_available_variant.image | json }},
                "inventory_quantity": {{ product.selected_or_first_available_variant.inventory_quantity | json }},
                "options": {{ product.selected_or_first_available_variant.options | default: empty_array | json }},
                "title": {{ product.selected_or_first_available_variant.title | default: "" | json }}
