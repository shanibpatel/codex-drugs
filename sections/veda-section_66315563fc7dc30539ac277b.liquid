                  "featured_image": {{ selectedItem.featured_image | json }},
                  "featured_media": {{ selectedItem.featured_media | default: empty_array | json }},
                  "image": {{ selectedItem.image | json }},
                  "inventory_quantity": {{ selectedItem.inventory_quantity | json }},
                  "options": {{ selectedItem.options | default: empty_array | json }},
                  "title": {{ selectedItem.title | json }}
                  {% if selectedItem.sku %}
