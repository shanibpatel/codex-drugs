            <span><i class="fal fa-angle-right"></i></span>
          </div>
        </div>
        {% if section.settings.VEDA_switch_8 %}
          <div
            class="veda-swiper-thumbnails w:100%"
            style="margin-top: {{ gap }}px"
            data-options="
              {
                spaceBetween: {{ gap }},
                {% if section.settings["VEDA_slider_8"] %}
                  slidesPerView: {{ section.settings["VEDA_slider_8"] }}
                {% else %}
                  slidesPerView: 6
                {% endif %}
              }
            "
          >
            <div class="swiper">
              <div class="swiper-wrapper">
                {% for media1 in product.media %}
                  {% if media1.media_type == 'video' or media1.media_type == 'external_video' %}
