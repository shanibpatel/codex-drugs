{% assign uniqueId = 'id_85c4d86f-029c-4de5-9e44-b8dd39d7951a' %}
{% assign builderMode = false %}
{% assign envatoMode = false %}
{% liquid
  if section.settings.VEDA_switch_1
    assign backgroundFixed = 'fixed'
  else
    assign backgroundFixed = 'scroll'
  endif

  if section.settings.VEDA_select_2 == 'color'
    assign backgroundColor = 'background-color: ' | append: section.settings.VEDA_color_1 | append: ';'
  else
    assign backgroundColor = ''
  endif

  if section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2
    assign bgImage = section.settings.VEDA_image_2 | append: '&width=2000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  elsif section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2 == blank
    assign bgImage = 'https://cdn.shopify.com/s/files/1/0552/5510/5616/files/udxhtnzxtq.png?v=1681903122&width=1000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  else
    assign backgroundImage = ''
  endif

  assign sectionPadding = 'padding-top: ' | append: section.settings.VEDA_space_4 | append: 'px;padding-right: ' | append: section.settings.VEDA_space_3 | append: 'px;padding-bottom: ' | append: section.settings.VEDA_space_1 | append: 'px;padding-left: ' | append: section.settings.VEDA_space_2 | append: 'px;'

  if section.settings.VEDA_select_1 == 'full_width'
    assign containerType = 'container-fluid veda-container-fluid'
  else
    assign containerType = 'container veda-container'
  endif

  if section.settings.VEDA_slider_1 >= 0
    assign containerGap = 'padding-left: ' | append: section.settings.VEDA_slider_1 | append: 'px; padding-right: ' | append: section.settings.VEDA_slider_1 | append: 'px;'
  else
    assign containerGap = ''
  endif

  if section.settings.section_settings___width >= 0 and section.settings.VEDA_select_1 == 'manual'
    assign containerWidth = 'max-width: ' | append: section.settings.section_settings___width | append: 'px;'
  else
    assign containerWidth = ''
  endif

  if section.settings.VEDA_slider_2 >= 0
    assign settingGap = section.settings.VEDA_slider_2 | divided_by: 2.0

    assign settingGapVertical = section.settings.VEDA_slider_2
  else
    assign settingGap = ''

    assign settingGapVertical = ''
  endif

  assign colGap = 'margin-top: ' | append: settingGapVertical | append: 'px; padding-left: ' | append: settingGap | append: 'px; padding-right: ' | append: settingGap | append: 'px;'

  assign rowGap = 'margin-top: -' | append: settingGapVertical | append: 'px; margin-left: -' | append: settingGap | append: 'px; margin-right: -' | append: settingGap | append: 'px;'

  assign sectionSeparator = ''

  if section.settings.VEDA_switch_3
    if section.settings.VEDA_slider_4
      assign sectionSeparator = sectionSeparator | append: 'border-top: ' | append: section.settings.VEDA_slider_4 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif

    if section.settings.VEDA_slider_5
      assign sectionSeparator = sectionSeparator | append: 'border-bottom: ' | append: section.settings.VEDA_slider_5 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif
  endif

  if colGap and rowGap and builderMode and envatoMode
  endif
%}

{% liquid
  assign _gap = false

  assign _class = false

  assign _component = false

  assign _design = false

  assign _button_class = false

  assign _name = false

  assign _form = false

  assign _data_confirm_message = false

  assign _data_target = false

  assign _aria_label = false

  assign _data_index = false

  assign _data_form_active = false

  assign _id = false

  assign _tooltip_position = false

  assign _tooltip_text = false

  assign _data_button_video_id = false

  assign _icon_class = false
%}
{% capture Image %}
  {% if product %}
    {% liquid
      assign imageXs = 12 | minus: section.settings.responsive___content__xs

      assign imageSm = 12 | minus: section.settings.responsive___content__sm

      assign imageMd = 12 | minus: section.settings.responsive___content__md

      assign imageLg = 12 | minus: section.settings.responsive___content__lg

      if section.settings.veda_sticky == 'media_sticky'
        assign mediaSticky = 'veda-product-sticky pos:sticky@md! t:10px@md h:fit-content@md z:999 mb:15px@+md'
      else
        assign mediaSticky = ''
      endif
    %}
    <div
      {% if section.settings.product_media___position == 'left' %}
        class="col-xs-{{ imageXs }} col-sm-{{ imageSm }} col-md-{{ imageMd }} col-lg-{{ imageLg }} d:flex fld:column ai:center pr:pfs(15px,30px)@md addon-disabled {{ mediaSticky }}"
      {% else %}
        class="col-xs-{{ imageXs }} col-sm-{{ imageSm }} col-md-{{ imageMd }} col-lg-{{ imageLg }} d:flex fld:column ai:center pl:pfs(15px,30px)@md addon-disabled {{ mediaSticky }}"
      {% endif %}
      {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
        style="{{ colGap }}"
      {% endif %}
    >
      {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
      {% liquid
        assign image_length = product.media | size

        assign gap = 10

        if section.settings.product_media___gap
          assign gap = section.settings.product_media___gap
        endif

        assign zoom_type = section.settings.VEDA_select_6 | default: 'lightbox_hover'
      %}
      {% if image_length <= 1 %}
        <div class="pos:relative w:100% z:1">
          {% for mediaItem in product.media %}
            {% if mediaItem.media_type == 'video' %}
              <div class="w:100% h:auto">
                <video
                  loop
                  controls
                  muted
                  playsinline
                  class="w:100%"
                  src="{% if mediaItem.media_type == 'video' %}{{ mediaItem.sources[1].url }}{% else %}{{ mediaItem | external_video_url }}{% endif %}"
                ></video>
              </div>
            {% elsif mediaItem.media_type == 'external_video' %}
              <div class="w:100% pos:relative" style="padding-top: calc(100% / {{ mediaItem.aspect_ratio }})">
                {% if mediaItem.host == 'youtube' %}
                  <iframe
                    src="https://www.youtube.com/embed/{{ mediaItem.external_id }}?enablejsapi=1&autoplay=1&loop=1"
                    class="js-youtube pos:absolute t:0 l:0 w:100% h:100%"
                    allow="autoplay; encrypted-media"
                    allowfullscreen
                  ></iframe>
                {% else %}
                  <iframe
                    src="https://player.vimeo.com/video/{{ mediaItem.external_id }}?autoplay=1&loop=1"
                    class="js-vimeo pos:absolute t:0 l:0 w:100% h:100%"
                    allow="autoplay; encrypted-media"
                    allowfullscreen
                  ></iframe>
                {% endif %}
              </div>
            {% elsif mediaItem.media_type == 'model' %}
              <div class="product-media-modal__model">
                <product-model
                  class="deferred-media veda-media media--transparent global-media-settings global-media-settings--no-shadow"
                  style="padding-top: min(calc(100vh - 12rem), 100%)"
                >
                  <button id="Deferred-Poster-Modal-{{ mediaItem.id }}" class="deferred-media__poster" type="button">
                    <span class="deferred-media__poster-button motion-reduce">
                      <svg
                        xmlns="http://www.w3.org/2000/svg"
                        aria-hidden="true"
                        focusable="false"
                        class="icon icon-3d-model"
                        fill="none"
                        viewBox="0 0 18 21"
                      >
                        <path d="M7.67998 20.629L1.28002 16.723C0.886205 16.4784 0.561675 16.1368 0.337572 15.731C0.113468 15.3251 -0.00274623 14.8686 -1.39464e-05 14.405V6.59497C-0.00238367 6.13167 0.113819 5.6755 0.33751 5.26978C0.561202 4.86405 0.884959 4.52227 1.278 4.27698L7.67796 0.377014C8.07524 0.131403 8.53292 0.000877102 8.99999 9.73346e-08C9.46678 -0.000129605 9.92446 0.129369 10.322 0.374024V0.374024L16.722 4.27399C17.1163 4.51985 17.4409 4.86287 17.6647 5.27014C17.8885 5.67742 18.0039 6.13529 18 6.59998V14.409C18.0026 14.8725 17.8864 15.3289 17.6625 15.7347C17.4386 16.1405 17.1145 16.4821 16.721 16.727L10.321 20.633C9.92264 20.8742 9.46565 21.0012 8.99999 21C8.53428 20.9998 8.07761 20.8714 7.67998 20.629V20.629ZM8.72398 2.078L2.32396 5.97803C2.22303 6.04453 2.14066 6.13551 2.08452 6.24255C2.02838 6.34959 2.00031 6.46919 2.00298 6.59003V14.4C2.00026 14.5205 2.02818 14.6396 2.08415 14.7463C2.14013 14.853 2.22233 14.9438 2.32298 15.01L7.99999 18.48V10.919C8.00113 10.5997 8.08851 10.2867 8.25292 10.0129C8.41732 9.73922 8.65267 9.51501 8.93401 9.36401L15.446 5.841L9.28001 2.08002C9.19614 2.02738 9.09901 1.99962 8.99999 2C8.90251 1.99972 8.8069 2.02674 8.72398 2.078V2.078Z" fill="currentColor"/>
                      </svg>
                    </span>
                    <img
                      srcset="
                        {% if mediaItem.preview_image.width >= 288 %}{{ mediaItem.preview_image | image_url: width: 288 }} 288w,{% endif %}
                        {% if mediaItem.preview_image.width >= 576 %}{{ mediaItem.preview_image | image_url: width: 576 }} 576w,{% endif %}
                        {% if mediaItem.preview_image.width >= 550 %}{{ mediaItem.preview_image | image_url: width: 550 }} 550w,{% endif %}
                        {% if mediaItem.preview_image.width >= 1100 %}{{ mediaItem.preview_image | image_url: width: 1100 }} 1100w,{% endif %}
                        {{ mediaItem.preview_image | image_url }} {{ mediaItem.preview_image.width }}w
                      "
                      src="{{ media | image_url: width: 550, height: 550 }}"
                      sizes="(min-width: 1200px) calc((1200px - 10rem) / 2), (min-width: 750px) calc((100vw - 11.5rem) / 2), calc(100vw - 4rem)"
                      width="576"
                      height="{{ 576 | divided_by: mediaItem.preview_image.aspect_ratio }}"
                      alt="{{ mediaItem.preview_image.alt | escape }}"
                    >
                  </button>
                  <template>{{ mediaItem | media_tag: image_size: '2048x', toggleable: true }}</template>
                </product-model>
                <button
                  class="button button--full-width product__xr-button"
                  type="button"
                  aria-label="{{ 'veda.products.product.xr_button_label' | t }}"
                  data-shopify-xr
                  data-shopify-model3d-id="{{ mediaItem.id }}"
                  data-shopify-title="title"
                  data-shopify-xr-hidden
                >
                  {{ 'veda.products.product.xr_button' | t }}
                </button>
              </div>
            {% else %}
              <div
                {% if mediaItem.preview_image.src != blank %}
                  {% if zoom_type == 'lightbox_hover' or zoom_type == 'hover' %}
                    class="veda-image-zoom w:100% h:auto pos:relative"
                  {% endif %}
                {% else %}
                  class="w:100% h:auto pos:relative"
                {% endif %}
              >
                {% if mediaItem.preview_image.src != blank %}
                  {% if zoom_type == 'lightbox_hover' or zoom_type == 'lightbox' %}
                    <a
                      class="d:flex jc:center ai:center pos:absolute t:0 l:0 w:100% h:100% clp:rect(20px,50px,50px,20px) trf:rotateY(180deg) cur:pointer z:999"
                      href="{{ mediaItem.preview_image.src | image_url: width: mediaItem.preview_image.width }}"
                      data-pswp-width="{{ mediaItem.preview_image.width }}"
                      data-pswp-height="{{ mediaItem.preview_image.height }}"
                    >
                      <span class="w:30px h:30px bgc:color-light pos:absolute t:20px l:20px d:flex jc:center ai:center bdrs:50%">
                        <i class="fal fa-search-plus fz:18px"></i>
                      </span>
                      <img
                        class="w:100% pos:absolute ins:0 op:0 bgc:color-light-freeze"
                        src="{{ mediaItem.preview_image.src | image_url: width: mediaItem.preview_image.width }}"
                        alt="{{ variantItem.title }}"
                        width="{{ mediaItem.preview_image.width }}"
                        height="{{ mediaItem.preview_image.height }}"
                        data-intrinsic-width="{{ mediaItem.preview_image.width }}"
                        data-intrinsic-height="{{ mediaItem.preview_image.height }}"
                      >
                    </a>
                  {% endif %}
                {% endif %}
                <div
                  {% if zoom_type == 'lightbox_hover' or zoom_type == 'hover' %}
                    class="d:block w:100% cur:zoom-in"
                  {% else %}
                    class="d:block w:100%"
                  {% endif %}
                >
                  {% if mediaItem.preview_image.src != blank %}
                    <img
                      class="veda-lazyload w:100%"
                      {% if zoom_type == 'lightbox_hover' or zoom_type == 'hover' %}
                        data-image-zoom-src="{{ mediaItem.preview_image.src | image_url: width: mediaItem.preview_image.width }}"
                        data-image-zoom="2"
                      {% endif %}
                      src="{{ mediaItem.preview_image.src | image_url: width: mediaItem.preview_image.width }}"
                      alt="{{ variantItem.title }}"
                      width="{{ mediaItem.preview_image.width }}"
                      height="{{ mediaItem.preview_image.height }}"
                      data-intrinsic-width="{{ mediaItem.preview_image.width }}"
                      data-intrinsic-height="{{ mediaItem.preview_image.height }}"
                    >
                  {% else %}
                    {{ 'image' | placeholder_svg_tag: 'veda-placeholder-svg bgc:color-gray2 ' }}
                  {% endif %}
                </div>
              </div>
            {% endif %}
          {% endfor %}
        </div>
      {% else %}
        <div
          class="veda-swiper-media pos:relative w:100% z:1 veda-photo-swipe"
          data-options="
            {
              speed: 400,
              centeredSlides: false,
              swiperAutoHeight: true,
              slideActiveClass: 'swiper-container',
            }
          "
        >
          <div class="swiper">
            <div class="swiper-wrapper">
              {% for media in product.media %}
                {% if media.media_type == 'video' %}
                  <div class="swiper-slide w:100% h:auto" data-media-id="{{ media.id }}">
                    <video
                      loop
                      controls
                      muted
                      playsinline
                      class="w:100%"
                      src="{% if media.media_type == 'video' %}{{ media.sources[1].url }}{% else %}{{ media | external_video_url }}{% endif %}"
                    ></video>
                  </div>
                {% elsif media.media_type == 'external_video' %}
                  <div class="swiper-slide w:100% h:auto" data-media-id="{{ media.id }}">
                    <div class="w:100% pos:relative" style="padding-top: calc(100% / {{ media.aspect_ratio }})">
                      {% if media.host == 'youtube' %}
                        <iframe
                          src="https://www.youtube.com/embed/{{ media.external_id }}?enablejsapi=1&autoplay=1&loop=1"
                          class="js-youtube pos:absolute t:0 l:0 w:100% h:100%"
                          allow="autoplay; encrypted-media"
                          allowfullscreen
                        ></iframe>
                      {% else %}
                        <iframe
                          src="https://player.vimeo.com/video/{{ media.external_id }}?autoplay=1&loop=1"
                          class="js-vimeo pos:absolute t:0 l:0 w:100% h:100%"
                          allow="autoplay; encrypted-media"
                          allowfullscreen
                        ></iframe>
                      {% endif %}
                    </div>
                  </div>
                {% elsif media.media_type == 'model' %}
                  <div class="swiper-slide product-media-modal__model" data-media-id="{{ media.id }}">
                    <product-model
                      class="deferred-media veda-media media--transparent global-media-settings global-media-settings--no-shadow"
                      style="padding-top: min(calc(100vh - 12rem), 100%)"
                    >
                      <button id="Deferred-Poster-Modal-{{ media.id }}" class="deferred-media__poster" type="button">
                        <span class="deferred-media__poster-button motion-reduce">
                          <svg
                            xmlns="http://www.w3.org/2000/svg"
                            aria-hidden="true"
                            focusable="false"
                            class="icon icon-3d-model"
                            fill="none"
                            viewBox="0 0 18 21"
                          >
